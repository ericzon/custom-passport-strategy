{
	"extends": ["airbnb-base", "prettier"],
	"plugins": ["prettier"],
	"rules": {
		"prefer-spread": "off",
		"comma-dangle": ["error", "never"],
		"no-underscore-dangle": "off",
		"class-methods-use-this": "off",
		"prefer-destructuring": "warn",
	  	"consistent-return": "warn",
		"prettier/prettier": [
			"error",
			{
				"printWidth": 100,
				"singleQuote": true
			}
		]
	}
}
